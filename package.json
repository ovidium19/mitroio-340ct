    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-sendfile": "^2.0.1",
    "koa-static": "^5.0.0",
    "less": "^3.8.1",
    "prop-types": "^15.6.2",
    "rc-progress": "^2.2.6",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-star-rating-component": "^1.4.1",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4",
    "underscore": "^1.9.1"
  },
