    "puppeteer": "^1.11.0",
    "react-test-renderer": "^16.6.1",
    "redux-devtools-extension": "^2.13.6",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "url-loader": "^1.1.2",
